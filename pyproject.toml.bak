[project]
name = "cloudoll"
version = "2.0.23"
authors = [
  { name="chuchur", email="chuchur@qq.com" },
]
description = "辅助快速创建可分布的微服务。"
readme = "README.md"
requires-python = ">=3.6"
license="MIT"
classifiers = [
    "License :: OSI Approved :: MIT License",
    "Programming Language :: Python",
    "Programming Language :: Python :: 3",
    "Programming Language :: Python :: 3.6",
    "Programming Language :: Python :: Implementation :: CPython",
    "Programming Language :: Python :: Implementation :: PyPy",
]
include_package_data=true

install_requires=[
    "requests",
    "colorlog",
    "aiomysql",
    "aiopg",
    "aiohttp",
    "redis[hiredis]",
    "aiomcache",
    "jinja2",
    "pyjwt",
    "aiohttp_session[secure]",
    "PyYAML",
    "click"
]
[project.urls]
Homepage = "https://gitee.com/chuchur/cloudoll-py"
Issues = "https://gitee.com/chuchur/cloudoll-py/issues"